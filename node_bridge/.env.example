# RPC URL for Sepolia (Infura/Alchemy)
RPC_URL=https://sepolia.infura.io/v3/YOUR_INFURA_PROJECT_ID

# Private key of the account that will sign the mint transactions (KEEP SECRET)
PRIVATE_KEY=0xYOUR_PRIVATE_KEY

# Deployed contract address
CONTRACT_ADDRESS=0xYourContractAddressHere

# For local Ganache test: two player accounts (private keys and addresses)
# Fill these with the private keys and addresses from your Ganache workspace.
PLAYER1_PRIVATE_KEY=0xPLAYER1_PRIVATE_KEY_FROM_GANACHE
PLAYER1_ADDRESS=0xPLAYER1_ADDRESS_FROM_GANACHE

PLAYER2_PRIVATE_KEY=0xPLAYER2_PRIVATE_KEY_FROM_GANACHE
PLAYER2_ADDRESS=0xPLAYER2_ADDRESS_FROM_GANACHE

# Optional: API key for nft.storage to upload metadata. If present, bridge will upload metadata JSON and use the IPFS URL as tokenURI.
NFT_STORAGE_API_KEY=YOUR_NFT_STORAGE_API_KEY
